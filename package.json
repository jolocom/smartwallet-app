{
  "name": "jolocomwallet",
  "version": "1.7.1",
  "scripts": {
    "run:ios": "react-native run-ios",
    "run:android": "react-native run-android --appIdSuffix debug",
    "build:ios": "cd ios && bundle exec fastlane build type:ad-hoc",
    "build:android": "cd android && bundle exec fastlane build local:true",
    "run:ios:staging": "cd ios && bundle exec fastlane staging",
    "run:android:staging": "cd android && bundle exec fastlane staging",
    "beta:ios": "cd ios && bundle exec fastlane beta",
    "beta:android": "cd android && bundle exec fastlane beta",
    "release:ios": "cd ios && bundle exec fastlane release",
    "release:android": "cd android && bundle exec fastlane release",
    "start": "adb reverse tcp:8081 tcp:8081 & react-native start",
    "test": "jest",
    "test:migrations": "bash ./tests/test_databases/test_migrations.sh",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "e2e:android": "detox build -c android.debug && detox test -c android.debug",
    "e2e:android:emu": "detox build -c android.emu.debug && detox test -c android.emu.debug",
    "e2e:ios:sim": "detox build -c ios.sim.debug && detox test -c ios.sim.debug",
    "clean:node": "rm -rf node_modules",
    "clean:android": "cd android && ./gradlew clean",
    "clean:ios": "cd ios && xcodebuild clean",
    "clean:react": "rm -rf $TMPDIR/react-native-packager-cache-* && rm -rf $TMPDIR/metro-bundler-cache-* && yarn start --reset-cache",
    "clean:all": "yarn run clean:ios & yarn run clean:android && yarn run clean:react && yarn run clean:node",
    "format": "eslint --fix --ext .ts --ext .tsx .",
    "icon": "svgr --native --out-dir src/resources/svg/",
    "devtools": "adb reverse tcp:8097 tcp:8097; react-devtools &",
    "terms": "ts-node -O '{\"module\": \"commonjs\"}' bin/generateTerms.ts",
    "storybook": "start-storybook -p 6006",
    "xcode": "xed ios/JolocomWallet.xcworkspace",
    "repl": "REPL=1 ts-node -O '{\"module\": \"commonjs\"}' -r ./bin/repl.ts",
    "typeorm": "ts-node -O '{\"module\": \"commonjs\"}' -r ./bin/repl.ts node_modules/.bin/typeorm",
    "depdraw": "ts-depdraw"
  },
  "private": true,
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/runtime": "^7.6.2",
    "@storybook/react-native": "^5.2.0-beta.30",
    "@svgr/cli": "^4.2.0",
    "@types/detox": "^14.5.2",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/i18n-js": "^3.0.1",
    "@types/jest": "^25.1.2",
    "@types/node": "^13.7.1",
    "@types/ramda": "^0.26.41",
    "@types/react": "^16.9.19",
    "@types/react-native": "^0.61.12",
    "@types/react-native-material-ui": "^1.32.0",
    "@types/react-native-snap-carousel": "^3.7.4",
    "@types/react-native-sqlite-storage": "^3.3.2",
    "@types/react-native-vector-icons": "^6.4.5",
    "@types/react-redux": "^7.1.7",
    "@types/react-test-renderer": "^16.9.2",
    "@types/redux-mock-store": "^1.0.2",
    "@types/sjcl": "^1.0.29",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "babel-jest": "^24.9.0",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-plugin-transform-typescript-metadata": "^0.2.2",
    "case-sensitive-paths-webpack-plugin": "^2.1.2",
    "class-transformer": "^0.1.9",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "3.0.2",
    "detox": "^15.2.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.18.3",
    "fs-extra": "^8.1.0",
    "husky": "^3.0.5",
    "jest": "^24.9.0",
    "material-colors": "^1.2.5",
    "metro-minify-terser": "^0.55.0",
    "metro-react-native-babel-preset": "^0.56.0",
    "mockdate": "^2.0.2",
    "node-fetch": "^2.6.0",
    "precise-commits": "^1.0.2",
    "prettier": "^1.18.2",
    "react-devtools": "^3",
    "react-dom": "^16.3.0",
    "react-test-renderer": "16.9.0",
    "redux-mock-store": "^1.5.1",
    "ts-depdraw": "^0.1.12",
    "ts-jest": "^24.0.2",
    "ts-loader": "3.5.0",
    "tsconfig-paths": "^3.8.0",
    "tslib": "^1.7.1",
    "typeorm": "^0.2.5",
    "typescript": "~3.7.0",
    "yarn": "^1.22.0"
  },
  "detox": {
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/JolocomWallet.app",
        "build": "xcodebuild -project ios/JolocomWallet.xcodeproj -scheme JolocomWallet -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone 7"
      },
      "android.emu.debug": {
        "binaryPath": "android/app/build/outputs/apk/debug/app-debug.apk",
        "build": "cd android && ./gradlew assembleDebug assembleAndroidTest -DtestBuildType=debug && cd ..",
        "type": "android.emulator",
        "name": "dynamic"
      },
      "android.emu.release": {
        "binaryPath": "android/app/build/outputs/apk/release/app-release.apk",
        "build": "cd android && ./gradlew assembleRelease assembleAndroidTest -DtestBuildType=release && cd ..",
        "type": "android.emulator",
        "name": "dynamic"
      },
      "android.debug": {
        "binaryPath": "android/app/build/outputs/apk/debug/app-debug.apk",
        "build": "cd android && ./gradlew assembleDebug assembleAndroidTest -DtestBuildType=debug && cd ..",
        "type": "android.attached",
        "name": "dynamic"
      },
      "android.release": {
        "binaryPath": "android/app/build/outputs/apk/release/app-release.apk",
        "build": "cd android && ./gradlew assembleRelease assembleAndroidTest -DtestBuildType=release && cd ..",
        "type": "android.attached",
        "name": "dynamic"
      }
    },
    "test-runner": "jest",
    "runner-config": "e2e/jest.config.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "precise-commits; node bin/precommit.js",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "dependencies": {
    "@react-native-community/art": "^1.1.2",
    "@react-native-community/async-storage": "^1.7.1",
    "@react-native-community/masked-view": "^0.1.1",
    "@sentry/react-native": "^1.3.3",
    "@types/react-native-modal-dropdown": "^0.6.1",
    "assert": "^2.0.0",
    "bip39": "^3.0.2",
    "browser-ipfs": "^0.0.6",
    "crypto-browserify": "^3.12.0",
    "ethereumjs-wallet": "^0.6.0",
    "events": "^3.0.0",
    "i18n-js": "^3.1.0",
    "jolocom-lib": "^4.1.0",
    "material-colors": "^1.2.5",
    "object.assign": "^4.1.0",
    "ramda": "^0.26.1",
    "react": "16.9.0",
    "react-native": "0.61.5",
    "react-native-camera": "^3.17.0",
    "react-native-crypto": "^2.1.2",
    "react-native-drawer": "^2.5.1",
    "react-native-gesture-handler": "^1.5.6",
    "react-native-indicator": "^1.2.0",
    "react-native-keychain": "^4.0.5",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-localize": "^1.3.3",
    "react-native-material-textfield": "^0.14.1",
    "react-native-material-ui": "^1.30.0",
    "react-native-modal-dropdown": "jolocom/react-native-modal-dropdown",
    "react-native-permissions": "^2.0.9",
    "react-native-qrcode-scanner": "^1.3.1",
    "react-native-randombytes": "^3.5.3",
    "react-native-reanimated": "^1.7.0",
    "react-native-safe-area-context": "^0.6.0",
    "react-native-screens": "^2.0.0-beta.2",
    "react-native-snap-carousel": "^3.8.0",
    "react-native-splash-screen": "^3.2.0",
    "react-native-sqlite-storage": "^4.1.0",
    "react-native-svg": "^11.0.1",
    "react-native-vector-icons": "^6.6.0",
    "react-native-version-number": "^0.3.6",
    "react-navigation": "^4.1.1",
    "react-navigation-animated-switch": "^0.4.1",
    "react-navigation-drawer": "^2.3.4",
    "react-navigation-stack": "^2.1.1",
    "react-navigation-tabs": "^2.7.0",
    "react-redux": "^7.2.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.3.0",
    "rn-fetch-blob": "^0.12.0",
    "sjcl": "^1.0.7",
    "sqlite3": "^4.0.0",
    "squel": "^5.12.1",
    "stream-browserify": "^2.0.2",
    "unorm": "^1.6.0",
    "vm-browserify": "^1.1.0",
    "wif": "^2.0.6"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
