{
  "name": "SmartWallet",
  "version": "2.2.0-040",
  "private": true,
  "scripts": {
    "android": "react-native run-android --appIdSuffix debug",
    "ios": "react-native run-ios",
    "ios:xcode": "open ./ios/SmartWallet.xcworkspace",
    "alpha:ios": "bundle exec fastlane ios alpha",
    "alpha:android": "bundle exec fastlane android alpha",
    "beta:ios": "bundle exec fastlane ios beta",
    "store:ios": "bundle exec fastlane ios store",
    "beta:android": "bundle exec fastlane android beta",
    "release:android": "bundle exec fastlane android release",
    "start": "adb reverse tcp:8081 tcp:8081 & react-native start",
    "term:add": "ts-node ./bin/terms/add-term.ts",
    "terms:import": "ts-node ./bin/terms/import-terms.ts",
    "terms:stale": "ts-node ./bin/terms/find-stale-terms.ts",
    "test-coverage": "jest --coverage --collectCoverageFrom=src/**/*.ts? --coverageDirectory=coverage",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:check": "eslint --ext .js,.jsx,.ts,.tsx -f ./bin/commit/formatter.js",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx -f table --fix",
    "prettier:check": "prettier --check",
    "prettier:checkAll": "prettier --check './**/*.ts|*.tsx'",
    "prettier:format": "prettier --write",
    "prettier:formatAll": "prettier --write './**/*.ts|*.tsx'",
    "devtools": "react-devtools",
    "clean:ios": "cd ios && xcodebuild clean",
    "clean:android": "cd android && ./gradlew clean",
    "clean:react": "rm -rf $TMPDIR/react-native-packager-cache-* && rm -rf $TMPDIR/metro-bundler-cache-* && yarn start --reset-cache",
    "clean:node": "rm -rf node_modules",
    "clean:all": "yarn run clean:ios & yarn run clean:android && yarn run clean:react && yarn run clean:node",
    "config:secrets": "ts-node ./bin/config-secrets.ts",
    "config:sentry": "ts-node ./bin/config-secrets.ts sentry"
  },
  "dependencies": {
    "@jolocom/native-core-react-native": "^1.3.0",
